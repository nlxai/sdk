    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/react": "^18.2.12",
    "rollup": "^3.25.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@nlxai/chat-core": "^1.0.5-alpha.13",
