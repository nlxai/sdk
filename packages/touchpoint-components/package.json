  "types": "./src/index.ts",
  "browser": "./dist/touchpoint-components.umd.js",
  "unpkg": "./dist/touchpoint-components.umd.js",
  "jsdelivr": "./dist/touchpoint-components.umd.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
