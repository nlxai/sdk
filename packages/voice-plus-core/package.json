{
  "name": "@nlxai/voice-plus-core",
  "version": "0.1.51",
  "description": "NLX SDK for Voice+ experiences",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.esm.js",
    "require": "./lib/index.cjs"
  },
  "scripts": {
    "build": "rm -rf lib && rollup -c --configPlugin typescript",
    "docs": "rm -rf docs/ && typedoc && concat-md --decrease-title-levels --dir-name-as-title docs/ > docs/index.md",
    "lint:check": "eslint src/ --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "prepublish": "npm run build",
    "preview-docs": "npm run docs && comrak --unsafe --gfm  -o docs/index.html docs/index.md && open docs/index.html",
    "publish-docs": "npm run docs && mv docs/index.md ../website/src/content/06-03-voice-plus-api-reference.md",
    "test": "jest && typedoc --emit none",
    "tsc": "tsc"
  },
  "author": "Peter Szerzo <peter@nlx.ai>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.1",
    "@types/ramda": "0.29.1",
    "concat-md": "^0.5.1",
    "eslint-config-nlx": "*",
    "fast-check": "^3.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rollup": "^4.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "ramda": "^0.29.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "ddcee77185166c44ed1afc9a6073b51d22d6c6d1"
}
